<header class="flex flex-row flex-wrap">
	{% if entry.headerContent.logo|length %}
		{% set logo = entry.headerContent.one().optimizedImages %}
		{% set logoAltText = entry.headerContent.one().title %}
		{% include "_tacos-el-gallo/_snippets/logo" %}
	{% else %}
		{% include "_tacos-el-gallo/_snippets/site-title" %}
	{% endif %}
    {% set navList = entry.headerContent.navigation(':notempty:').all() %}
	{% include "_tacos-el-gallo/_content/nav-item-list" %}
    {% set socialMediaList = entry.headerContent.socialMediaAccounts(':notempty:').all() %}
	{% include "_tacos-el-gallo/_content/social-media-links" %}
</header>
